{
  "name": "node-service-a",
  "version": "1.0.0",
  "sourceRoot": "apps/node-service-a",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/node-service-a/dist",
        "main": "apps/node-service-a/app.ts",
        "tsConfig": "apps/node-service-a/tsconfig.json",
        "assets": ["apps/node-service-a/src/assets"]
      }
    },
    "serve": {
      "executor": "@nrwl/node:serve",
      "options": {
        "buildTarget": "node-service-a:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/node-service-a"],
      "options": {
        "jestConfig": "apps/node-service-a/jest.config.js",
        "passWithNoTests": true
      }
    },
    "dev": {
      "executor": "@nrwl/node:serve",
      "options": {
        "buildTarget": "node-service-a:build",
        "watch": true
      }
    }
  }
}
